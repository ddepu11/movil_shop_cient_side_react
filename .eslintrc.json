{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": ["warn", { "endOfLine": "auto" }],
    "spaced-comment": "warn",
    "no-underscore-dangle": "warn"
  }
}
